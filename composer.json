{
    "name": "blackquadrant/capsule-crm-api-wrapper-php",
    "description": "A simple to use, standards compliant, exhaustive api wrapper for CapsuleCRM for PHP",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "hiqdev/composer-config-plugin": "^0.2.5"
    },
    "authors": [
        {
            "name": "Black Quadrant",
            "email": "dev@blackquadrant.com"
        }
    ],
    "autoload": {
        "psr-4": {"BlackQuadrant\\CapsuleCRM\\": "src/"}
    },
    "extra": {
        "config-plugin": {
            "api": "src/config/api.php"
        },
        "laravel": {
            "providers": "BlackQuadrant\\CapsuleCRM\\ServiceProvider"
        }
    }
}
